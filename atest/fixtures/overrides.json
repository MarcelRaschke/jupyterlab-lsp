{
  "@jupyterlab/statusbar-extension:plugin": {
    "visible": true
  },
  "@jupyter-lsp/jupyterlab-lsp:plugin": {
    "language_servers": {
      "pyright": {
        "priority": 1000
      },
      "pylsp-with-override-json": {
        "serverSettings": {
          "pylsp.plugins.flake8.enabled": true,
          "pylsp.plugins.pyflakes.enabled": false
        }
      }
    }
  }
}
